{
  "yaml.schemas": {
    "https://json.schemastore.org/github-workflow": [
      ".github/worflows/*.ya?ml"
    ],
    "https://raw.githubusercontent.com/ansible/ansible-lint/main/src/ansiblelint/schemas/ansible.json#/$defs/tasks": [
      "tasks/*.ya?ml",
      "handlers/*.ya?ml"
    ],
    "https://raw.githubusercontent.com/ansible/ansible-lint/main/src/ansiblelint/schemas/ansible.json#/$defs/playbook": [
      "run.ya?ml"
    ],
    "https://raw.githubusercontent.com/ansible/ansible-lint/main/src/ansiblelint/schemas/ansible.json#/$defs/inventory": [
      "inventory.ya?ml",
      "inventory/*.ya?ml",
      "hosts.ya?ml",
    ],
    "https://raw.githubusercontent.com/ansible/ansible-lint/main/src/ansiblelint/schemas/ansible.json#/$defs/requirements": [
      "requirements.ya?ml"
    ],
    "https://raw.githubusercontent.com/ansible/ansible-lint/main/src/ansiblelint/schemas/ansible.json#/$defs/vars": [
      "playbooks/vars/*.ya?ml",
      "vars/*.ya?ml",
      "defaults/*.ya?ml",
      "host_vars/*.ya?ml",
      "group_vars/*.ya?ml"
    ]
  },
  "yaml.customTags": [
    "!vault"
  ],
  "ansible.python.activationScript": ".vscode/ansible-ext-activate.sh",
  "ansible.python.interpreterPath": "ansible/.venv/bin/python3",
}